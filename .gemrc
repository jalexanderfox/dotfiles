# http://guides.rubygems.org/command-reference/#gem-environment
---
gem: --no-ri --no-rdoc
benchmark: false
verbose: true
update_sources: true
sources:
- http://gemcutter.org
- http://gems.rubyforge.org/
- http://gems.github.com/
- http://rubygems.org/
backtrace: true
bulk_threshold: 1000

